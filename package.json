{
  "name": "lance-game",
  "version": "2.0.0",
  "engines": {
    "node": "6.11.1"
  },
  "description": "An HTML5 multiplayer game based on Lance",
  "main": "main.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^13.0.1",
    "express": "^4.14.0",
    "lance-gg": "~1.0.0",
    "query-string": "^4.2.3",
    "socket.io": "^1.4.8",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babelify": "^7.3.0",
    "eslint": "^3.16.1",
    "eslint-config-google": "^0.7.1",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "start": "babel-node main.js --presets es2015",
    "nodemon": "nodemon main.js --exec babel-node --presets es2015",
    "postinstall": "webpack --display-error-details",
    "build": "webpack",
    "dev": "webpack --watch",
    "stats": "webpack --env production --profile --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "keywords": [
    "game",
    "lance",
    "lance-gg",
    "multiplayer",
    "websockets"
  ],
